{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/*",
    "src/share/**/*",
    "src/env.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "electron-vite/node"
    ],
    "baseUrl": ".",
    "paths": {
      "@share/*": [
        "src/share/*"
      ],
      "@resources/*":[
        "resources/*"
      ]
    }
  }
}